# RVC Project Environment Variables
# Copy this file to .env and fill in the values

# Path to directory containing trained model weights (.pth files)
# Example: assets/weights
weight_root=

# Path to UVR5 model weights directory
# Example: assets/uvr5_weights
weight_uvr5_root=

# Path to directory containing feature indices (.index files)
# Example: logs or assets/indices
index_root=

# Path to external index directory (optional)
# Example: /path/to/external/indices
outside_index_root=

# Path to RMVPE model directory
# Example: assets/rmvpe
rmvpe_root=
